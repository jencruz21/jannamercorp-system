                        </div>
                        <!-- end of eow -->
                    </div>
                </div>
                
            </div>
            <!-- Page-body end -->
        </div>
    </div>
    <!-- Main-body start -->




    <div class="card">
        <div class="card-header">
            <h5>Inventory List</h5>
            <!-- <span>use class <code>table-dark</code> inside table element</span> -->
            <div class="card-header-right">
                <ul class="list-unstyled card-option">
                    <li><i class="fa fa fa-wrench open-card-option"></i></li>
                    <li><i class="fa fa-window-maximize full-card"></i></li>
                    <li><i class="fa fa-minus minimize-card"></i></li>
                    <li><i class="fa fa-refresh reload-card"></i></li>
                    <li><i class="fa fa-trash close-card"></i></li>
                </ul>
            </div>
        </div>
        <div class="card-block table-border-style" style="overflow-y:scroll;height:500px;">
            <div class="table-responsive">
                <table class="table table-dark">
                   
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Date</th>
                            <th>Item</th>
                            <th>Type</th>
                            <th>Sacks</th>
                            <th>Retail</th>
                            <th>Less 5kg</th>
                            <th>Less 10kg</th>
                            <th>Market</th>
                            <th>TENC</th>
                            <th>W.S. Market</th>
                            <th>WholeSale</th>
                            <th>Special 1</th>
                            <th>Special 2</th>
                            <th>Divert</th>
                            <th>Buying Price</th>
                            <th>Total Kilos</th>
                            <th>Total Buying Price</th>
                            <th>Over All Price Retail</th>
                            <th>Gross</th>
                        
                        </tr>
                       
                    </thead>
                    <tbody>
                        <% for(var i = 0; i < stocks.length; i++) { %>
                        <tr>
                          
                            <th><%= i + 1 %></th>
                            <td><%= stocks[i].i_date %></td>
                           <td><%= stocks[i].i_item %></td>
                           <td><%= stocks[i].i_type %></td>
                           <td><%= stocks[i].i_sacks %></td>
                           <td><%= stocks[i].i_retail %></td>
                           <td><%= stocks[i].i_5kg %></td>
                           <td><%= stocks[i].i_10kg %></td>
                           <td><%= stocks[i].i_market %></td>
                           <td><%= stocks[i].i_tenC %></td>
                           <td><%= stocks[i].i_wsMarket %></td>
                           <td><%= stocks[i].i_wholeSale %></td>
                           <td><%= stocks[i].i_special1 %></td>
                           <td><%= stocks[i].i_special2 %></td>
                           <td><%= stocks[i].i_divert %></td>
                           <td><%= stocks[i].i_buyingPrice %></td>
                           <td><%= stocks[i].i_totalKilos %></td>
                           <td><%= stocks[i].i_totalBuyingPrice %></td>
                           <td><%= stocks[i].i_overAllPriceRetail %></td>
                           <td><%= stocks[i].i_gross %></td>
                          
                      
                          
    
                         
    
                        </tr>
                        <% } %>
                
                     
                    </tbody>
                </table>
            </div>
        </div>
    </div>



<%- include('include/footer') %>