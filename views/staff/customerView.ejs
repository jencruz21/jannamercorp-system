                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_route %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_previousRemitance %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_isCleared %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_creditLimit %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_transaction %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_addons %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_less %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_cashRemited %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_checkValue %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_checkNumber %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_noteTransaction %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_itemSubtotal %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_grandTotal %></td>
                            <td style="background-color: rgba(42, 92, 241, 0.418);color: yellowgreen;"><%= orderingDetails[i].od_cavans %></td>
