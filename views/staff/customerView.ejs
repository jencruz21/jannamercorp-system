  </div>
  <!-- Main-body start -->




  <div class="card">
      <div class="card-header">
          <h5>Customers List</h5>
          <!-- <span>use class <code>table-dark</code> inside table element</span> -->
          <div class="card-header-right">
              <ul class="list-unstyled card-option">
                  <li><i class="fa fa fa-wrench open-card-option"></i></li>
                  <li><i class="fa fa-window-maximize full-card"></i></li>
                  <li><i class="fa fa-minus minimize-card"></i></li>
                  <li><i class="fa fa-refresh reload-card"></i></li>
                  <li><i class="fa fa-trash close-card"></i></li>
              </ul>
          </div>
      </div>
      <div class="card-block table-border-style" style="overflow-y:scroll;height:500px;">
          <div class="table-responsive">
              <table class="table table-dark">
                 
                  <thead>
                      <tr>
                          <th>#</th>
                          <th>Date</th>
                          <th>Customers Name</th>
                          <th>Contact #</th>
                          <th>Location</th>
                          <th>Item Name</th>
                          <th>Price</th>
                          <th>Sacks</th>
                          <th>Kilos</th>
                          <th>Amount</th>
                          <th>Route</th>
                          <th>Previous Remitance</th>
                          <th>Is Cleared</th>
                          <th>Credit Limit</th>
                          <th>Transaction</th>
                          <th>Addons</th>
                          <th>Less</th>
                          <th>Cash Remited</th>
                         
                          <th>Check Value</th>
                          <th>Check Number</th>
                          <th>Notes</th>
                          <th>Item Subtotal</th>
                          <th>Grand Total</th>
                          <th> Cavans</th>
                          <th>Change</th>
                          <th>New Status</th>
                          <th>Rows Left</th>
                          <th>New Balance</th>
                          <th>Remarks</th>
                          <th>Valid ID</th>
                          
                      </tr>
                     
                  </thead>
                  <tbody>
                    <% for(var i = 0; i <orderingDetails.length; i++) { %>
                        <tr>
                          
                            <th><%= i + 1 %></th>

                            <td><%= orderingDetails[i].od_date %></td>
                            <td><%= orderingDetails[i].od_customer %></td>
                             <td><%= orderingDetails[i].od_number %></td> 
                             <td><%= orderingDetails[i].od_location %></td>  
                            <td><%= orderingDetails[i].od_itemName %></td>
                            <td><%= orderingDetails[i].od_selectPricing %></td>
                            <td><%= orderingDetails[i].od_sacks %></td>
                            <td><%= orderingDetails[i].od_kilos %></td>
                            <td><%= orderingDetails[i].od_amount %></td>
                            <td><%= orderingDetails[i].od_route %></td>
                            <td><%= orderingDetails[i].od_previousRemitance %></td>
                            <td><%= orderingDetails[i].od_isCleared %></td>
                            <td><%= orderingDetails[i].od_creditLimit %></td>
                            <td><%= orderingDetails[i].od_transaction %></td>
                            <td><%= orderingDetails[i].od_addons %></td>
                            <td><%= orderingDetails[i].od_less %></td>
                            <td><%= orderingDetails[i].od_cashRemited %></td>
                            <td><%= orderingDetails[i].od_checkValue %></td>
                            <td><%= orderingDetails[i].od_checkNumber %></td>
                            <td><%= orderingDetails[i].od_noteTransaction %></td>
                            <td><%= orderingDetails[i].od_itemSubtotal %></td>
                            <td><%= orderingDetails[i].od_grandTotal %></td>
                            <td><%= orderingDetails[i].od_cavans %></td>
                            <td><%= orderingDetails[i].od_change %></td>
                            <td><%= orderingDetails[i].od_newStatus %></td>
                            <td><%= orderingDetails[i].od_rowsLeft %></td>
                            <td><%= orderingDetails[i].od_newBalance %></td>
                            <td><%= orderingDetails[i].od_remarks %></td>
                            <td><%= orderingDetails[i].od_validId %></td>
                       

                            <td><%= orderingDetails[i].i_sacks %></td>


                          
    
                     
    
                        </tr>
                        <% } %>
                
                     
              
                   
                  </tbody>
              </table>
          </div>
      </div>
  </div>
  

  <div id="styleSelector">

  </div>
</div>
</div>
</div>
</div>
</div>
</div>





