            <h1 class="m-0">JANNAMER CORPORATION</h1>
        </a>
        <button type="button" class="navbar-toggler me-0" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto p-4 p-lg-0">

                <!-- Home  -->
                <% if (page === "/") { %>
                    <a href="/" class="nav-item nav-link active">Home</a>
                <% } else { %>
                    <a href="/" class="nav-item nav-link">Home</a>
                <% } %>

                <!-- About -->
                <% if (page === "/about") { %>
                    <a href="/about" class="nav-item nav-link active">About</a>
                <% } else { %>
                    <a href="/about" class="nav-item nav-link">About</a>
                <% } %>

                <!-- Contact -->
                <% if (page === "/contact") { %>
                    <a href="/contact" class="nav-item nav-link active">Contact Us</a>
                <% } else { %>
                    <a href="/contact" class="nav-item nav-link">Contact Us</a>
                <% } %>

                <!-- Services -->
                <% if (page === "/products") { %>
                    <a href="/products" class="nav-item nav-link active">Sample Price List</a>
                <% } else { %>
                    <a href="/products" class="nav-item nav-link">Sample Price List</a>
                <% } %>

                <!-- Products -->
                <% if (page === "/company-view") { %>
                    <a href="/company-view" class="nav-item nav-link active">Company View</a>
                <% } else { %>
                    <a href="/company-view" class="nav-item nav-link">Company View</a>
                <% } %>
            </div>
        </div>
    </nav>
